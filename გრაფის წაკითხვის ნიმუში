#include <bits/stdc++.h>
using namespace std;
int n,m,a,b,w;
map < pair<int,int>, int> mp;
vector<int>v[100005];
int main (){
    cin>>n>>m;
    while (m--)    {
        cin>>a>>b>>w;
        v[a].push_back(b);  v[b].push_back(a);
        mp[{a,b}]=w;        mp[{b,a}]=w;
}   }
