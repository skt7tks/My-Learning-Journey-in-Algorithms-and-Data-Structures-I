#include <bits/stdc++.h>
using namespace std;
int n, m, v, u, w;
vector<pair<int, int>> g[100100];
main() {
    cin >> n >> m;
    for (int i = 0; i < m; ++i) {
        cin >> v >> u >> w; 
        g[v].push_back({ u, w });  g[u].push_back({ v, w });
}   }
